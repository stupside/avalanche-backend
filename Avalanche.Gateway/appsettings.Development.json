{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "LettuceEncrypt": {
    "AcceptTermsOfService": true,
    "DomainNames": [
      "localhost"
    ],
    "EmailAddress": "it@avalanche.com"
  },
  "Identity": {
    "ClientIntrospection": {
      "Issuer": "http://localhost:5678",
      "Audiences": [
        "avalanche"
      ],
      "ClientId": "avalanche",
      "ClientSecret": "avalanche"
    },
    "ClientRegistration": {
      "Issuer": "http://localhost:5678",
      "ClientId": "avalanche",
      "ClientSecret": "avalanche"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "business": {
        "ClusterId": "business",
        "AuthorizationPolicy": "anonymous",
        "Match": {
          "Path": "/business"
        }
      }
    },
    "Clusters": {
      "business": {
        "Destinations": {
          "business/destination1": {
            "Address": "http://localhost:8001"
          }
        }
      }
    }
  }
  
}
