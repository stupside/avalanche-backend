{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "http": {
        "Url": "http://+:8000"
      }
    },
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "ConnectionStrings": {
    "Npgsql": "Host=database.avalanche;Port=5432;Database=avalanche-business;Username=user;Password=password"
  },
  "RabbitMQOptions": {
    "HostName": "rabbitmq",
    "Port": 5672,
    "VirtualHost": "/",
    "UserName": "user",
    "Password": "password"
  }
}
